<h1>List of Publishers:</h1>
<h2>Add Publisher</h2>

<table>
    <thead> 
        <th>Publisher Name</th> 
    </thead> 

    <tbody> 
        {{#each publisher}} 
        <tr> 
            <td>{{name}}</td>
        </tr> 
        {{/each}} 
    </tbody> 

    <form id="addpublisher" action="/publisher" method="post">
    Publisher Name: <input type="text" name="publishername"><br>
    <input type="submit" value="Submit">
    </form>
</table> 
