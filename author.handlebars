<h1>List of Authors:</h1>
<h2>Add Author</h2>

<table>
    <thead> 
        <th>Author Name</th> 
        <th>Date of Birth</th> 
    </thead> 

    <tbody> 
        {{#each author}} 
        <tr> 
            <td>{{name}}</td> 
            <td>{{dob}}</td> 
        </tr> 
        {{/each}} 
    </tbody> 

    <form id="addauthor" action="/author" method="post">
    Author Name: <input type="text" name="authorname"><br>
    Date of Birth: <input type="date" name="authordob"><br>
    <input type="submit" value="Submit">
    </form>
</table> 
